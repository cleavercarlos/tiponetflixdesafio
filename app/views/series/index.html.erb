
<h1>Series</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Synopsis</th>
      <th>Director</th>
    </tr>
  </thead>
  <tbody>
    <% @series.each do |serie| %>
      <tr>
        <td><%= serie.name %></td>
        <td><%= serie.synopsis %></td>
        <td><%= serie.director %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Serie', new_series_path %>
